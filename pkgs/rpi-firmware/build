
project=raspberrypi
version=e4c0a4d6f5343d5debf8dbc1966f046541572ffd
url=https://github.com/raspberrypi/rpi-firmware/archive/${version}.tar.gz
checksum=9bbaf290de3ce815399024e39e32fed85b23c1e79fadbf97421a0587cda840a5

build() {
  mkdir -p ${BOOTPART}/boot
  cp start4.elf fixup4.dat ${BOOTPART}/
  cp -r overlays ${BOOTPART}/boot/
}
