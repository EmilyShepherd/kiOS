version=1.35.0
url=https://busybox.net/downloads/busybox-${version}.tar.bz2
checksum=faeeb244c35a348a334f4a59e44626ee870fb07b6884d68c10ae8bc19f83a694
type=make
initrd_binaries="/sbin/modprobe /usr/bin/nsenter /bin/mount /bin/umount"

pre_build() {
  if ! test -f .config
  then
    sed "s#BUILD_DIR#${BUILD_DIR}#" ../config > .config
  fi
}
