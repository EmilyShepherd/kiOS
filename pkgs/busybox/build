version=1.34.1
url=https://busybox.net/downloads/busybox-${version}.tar.bz2
checksum=415fbd89e5344c96acf449d94a6f956dbed62e18e835fc83e064db33a34bd549
type=make
initrd_binaries="/bin/sh /sbin/modprobe /usr/bin/nsenter /bin/mount /bin/umount"

pre_build() {
  if ! test -f .config
  then
    sed "s#BUILD_DIR#${BUILD_DIR}#" ../config > .config
  fi
}
