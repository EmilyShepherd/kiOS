
version=1.3
url=https://www.lysator.liu.se/~nisse/misc/argp-standalone-${version}.tar.gz
checksum=dec79694da1319acd2238ce95df57f3680fea2482096e483323fddf3d818d8be
type=configure
optimise=O0

post_build() {
  mkdir -p ${BUILD_DIR}/{lib,include}
  cp argp.h ${BUILD_DIR}/include/
  cp libargp.a ${BUILD_DIR}/lib/
}
